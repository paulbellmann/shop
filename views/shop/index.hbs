{{# each products }}
    <div class="row">
        {{# each this }}
            <div class="col-sm-6 col-md-4">
              <div class="thumbnail">
                <img src="{{ imagePath }}" alt="..." class="img-responsive">
                <div class="caption">
                  <h3>{{ title }}</h3>
                  <p class="description">{{ description }}</p>
                  <div class="clearfix">
                      <div class="price pull-left">{{ price }}$</div>
                      <a href="#" class="btn btn-success pull-right" role="button">Add</a>
                  </div>
                </div>
              </div>
            </div>
        {{/each }}
    </div>
{{/each }}